/************************* ARMADILHAS *************************/
/*input dos dados de armadilhas*/
FILENAME REFFILE '/folders/myfolders/sasuser.v94/TCC/Armadilhas.xlsx';

PROC IMPORT DATAFILE=REFFILE
	DBMS=XLSX
	OUT=tcc.armadilhas;
	GETNAMES=YES;
RUN;

PROC CONTENTS DATA=tcc.armadilhas; RUN;


/* ANOVA por medidas repetidas e teste adjacentes*/
PROC GLM DATA = tcc.armadilhas;
 CLASS trat bloco ;
  MODEL semana1 semana2 semana3 semana4 semana5 semana6 semana7 semana8 semana9= trat bloco/solution;
 REPEATED moscab polynomial/ PRINTE PRINTM;
 LSMEANS trat bloco; 
 output out=armadilhas p=pred1-pred9 r=r1-r9;
RUN;

/************** Armadilhas não paramétrico **************/
PROC GLM DATA = tcc.armadilhas_rank;
 CLASS trat bloco ;
 MODEL semana1 semana2 semana3 semana4 semana5 semana6 semana7 semana8 semana9= trat bloco/solution;
 REPEATED moscab polynomial/ PRINTE PRINTM;
 LSMEANS trat bloco; 
 output out=armadilhas_rank p=pred1-pred9 r=r1-r9;
RUN;



/************************* PLANTAS *************************/
/* ANOVA por medidas repetidas e teste adjacentes*/
PROC GLM DATA = tcc.plantas;
 CLASS trat bloco ;
 MODEL semana1 semana2 semana3 semana4 semana5 semana6 semana7= trat bloco trat*bloco/solution;
 REPEATED moscab polynomial/ PRINTE PRINTM;
 LSMEANS trat bloco trat*bloco; 
 output out=plantas_rank p=pred1-pred7 r=r1-r7;
RUN;

/*Não paramétrico*/
PROC GLM DATA = tcc.plantas_rank;
 CLASS trat bloco ;
 MODEL semana1 semana2 semana3 semana4 semana5 semana6 semana7= trat bloco trat*bloco/solution;
 REPEATED moscab polynomial/ PRINTE PRINTM;
 LSMEANS trat bloco trat*bloco; 
 output out=plantas_rank p=pred1-pred7 r=r1-r7;
RUN;











